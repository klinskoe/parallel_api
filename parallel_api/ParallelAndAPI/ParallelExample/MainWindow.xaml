<Window x:Class="ParallelExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParallelExample"
        mc:Ignorable="d"
        Title="Simple Parallel Example" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Width="100" Margin="0 10 0 0" Click="ParallelClick">Заполнить</Button>
        <ListBox Name="numbersListBox" Grid.Row="1" Margin="10"/>
    </Grid>
</Window>
